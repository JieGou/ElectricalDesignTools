<Window x:Class="WpfUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfUI"
        mc:Ignorable="d"
        Title="MainWindow" Width="1850" Height="900">
    <Grid>
        <DataGrid x:Name="dgdTest" HorizontalAlignment="Left" Height="608" 
                  Margin="403,152,0,0" VerticalAlignment="Top" Width="1404" 
                  AutoGeneratingColumn="OnAutoGeneratingColumn"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="27" Margin="111,188,0,0" VerticalAlignment="Top" Width="156"/>
        <ListBox HorizontalAlignment="Left" Height="105" Margin="111,305,0,0" VerticalAlignment="Top" Width="189" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="111,521,0,0" VerticalAlignment="Top"/>
        <Menu HorizontalAlignment="Left" Height="165" Margin="111,620,0,0" VerticalAlignment="Top" Width="189"/>
        <ComboBox HorizontalAlignment="Left" Height="14" Margin="111,442,0,0" VerticalAlignment="Top" Width="189"/>
        <!--ItemsSource="{Binding Path=loadList}"-->

    </Grid>
</Window>
