<Window x:Class="WpfUI.Windows.ApplicationSettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfUI.Windows"
        
        xmlns:appSettingsViews="clr-namespace:WpfUI.Views.ApplicationSettings"
        
        mc:Ignorable="d"
        Title="Application settings" Height="450" Width="800" 
        WindowStartupLocation="CenterOwner"
        
        
        Closed="Window_Closed"
        >
    <Grid>
        <appSettingsViews:AppSettingsView/>
    </Grid>
</Window>
