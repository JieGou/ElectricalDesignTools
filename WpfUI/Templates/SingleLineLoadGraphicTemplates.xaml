    <!--FdsSplitterTemplate-->
    <DataTemplate x:Key="FdsSplitterTemplate">
        <StackPanel>
            
            <Line Grid.Row="0" Grid.Column="1"
                   X1="39.5" X2="39.5" Y1="0" Y2="10"
                   Stroke="Black" StrokeThickness="1"/>
            <!--Dashed Border-->
            <Border BorderThickness="0,0,0,1">
                <Border.BorderBrush>
                    <DrawingBrush Viewport="0,0,8,8" ViewportUnits="Absolute" TileMode="Tile">
                        <DrawingBrush.Drawing>
                            <DrawingGroup>
                                <GeometryDrawing Brush="Black">
                                    <GeometryDrawing.Geometry>
                                        <GeometryGroup>
                                            <RectangleGeometry Rect="0,0,50,50" />
                                            <RectangleGeometry Rect="50,50,50,50" />
                                        </GeometryGroup>
                                    </GeometryDrawing.Geometry>
                                </GeometryDrawing>
                            </DrawingGroup>
                        </DrawingBrush.Drawing>
                    </DrawingBrush>
                </Border.BorderBrush>
            </Border>
            <Grid Style="{StaticResource GraphicHighlightStyle}"
                  SnapsToDevicePixels="True">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="18.5"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="auto"/>
                </Grid.RowDefinitions>

                
                <Line Grid.Row="0" Grid.Column="1"
                      X1="20.5" X2="20.5" Y1="0" Y2="10"
                      Stroke="Black" StrokeThickness="1"/>

                <Border BorderThickness="1" HorizontalAlignment="Center"
                        Grid.Row="1" Grid.Column="1"
                        Width="40" Height="60">

                    <Canvas Grid.Column="1" Width="50" Height="60">

                        <Line X1="20" X2="20" Y1="0" Y2="12.5"
                              Stroke="Black" StrokeThickness="1"/>
                        <Line X1="10" X2="20" Y1="12.5" Y2="23"
                              Stroke="Black" StrokeThickness="1"/>
                        <Line X1="20" X2="20" Y1="23" Y2="30"
                              Stroke="Black" StrokeThickness="1"/>
                        <Rectangle Width="10.5" Height="20"
                                    Canvas.Left="14.5" Canvas.Top="30"
                                    Stroke="Black" StrokeThickness="1"/>
                        <Line X1="20" X2="20" Y1="50" Y2="60"
                              Stroke="Black" StrokeThickness="1"/>
                    </Canvas>
                </Border>



                
                <Border Grid.Row="1" Grid.Column="2"
                        Grid.RowSpan="2">
                    <StackPanel Orientation="Vertical"
                                VerticalAlignment="Top">
                        <TextBlock Text="{Binding Tag}"
                                    FontWeight="SemiBold"
                                    Margin="5 0 0 0"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="{Binding PdSizeTrip, StringFormat={}{0:N0} AF}"
                                    Margin="5 0 0 0"/>
                            <TextBlock Text="/"
                                    Margin="5 0 0 0"/>
                            <TextBlock Text="{Binding PdSizeFrame, StringFormat={}{0:N0} AT}"
                                    Margin="5 0 0 0"/>
                        </StackPanel>
                        <!--Area Properties-->
                        <StackPanel Orientation="Horizontal" 
                                    Visibility="Visible">
                            <TextBlock Text="" Margin="5 0 0 0"/>
                            <TextBlock Text="{Binding NemaRating}"/>
                            <TextBlock Text=", " Margin="0 0 0 0"/>
                            <TextBlock Text="{Binding AreaClassification}" />
                        </StackPanel>

                        
                        <!--Extra Details-->
                        <StackPanel x:Name="extraDetails"
                                    Orientation="Vertical">
                            <!--Area Tag-->
                            <TextBlock Text="{Binding Area.Tag}"
                                       FontWeight="SemiBold"
                                       Margin="5 0 0 0"
                                       Visibility="Collapsed"/>
                            <!--Area Properties-->
                            <StackPanel Orientation="Horizontal" 
                                        Visibility="Collapsed">
                                <TextBlock Text="" Margin="5 0 0 0"/>
                                <TextBlock Text="{Binding NemaRating}"/>
                                <TextBlock Text=", " Margin="0 0 0 0"/>
                                <TextBlock Text="{Binding AreaClassification}" />
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </Border>
                
                <Border BorderThickness="0 0 1 0" HorizontalAlignment="Center"
                        Grid.Row="3" Grid.Column="1"
                        Margin="2 0 1 0"
                        Height="{Binding ActualHeight, ElementName=extraDetails}">
                </Border>

            </Grid>
            <Line Grid.Row="0" Grid.Column="1"
                    X1="39.5" X2="39.5" Y1="0" Y2="10"
                              Stroke="Black" StrokeThickness="1"
                  />
           
        </StackPanel>
    </DataTemplate>


    <!--DRIVE-->
    <!--Vsd-->
    <DataTemplate x:Key="VsdTemplate">
        <Grid Background="Transparent"
                      Style="{StaticResource GraphicHighlightStyle}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="13.5"/>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>


            <Border BorderThickness="1" HorizontalAlignment="Center"
                    Grid.Row="0" Grid.Column="1"
                    Width="50.5" Height="50">
                <TextBlock Text="{Binding Type}"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"/>
            </Border>

            <!--Delete this border when enabling additional tags to allow expanding-->
            <Border Grid.Row="0" Grid.Column="2"
                    Grid.RowSpan="2"
                    CornerRadius="{StaticResource BorderHighlightCornerRadius}"
                    Padding="0 0 5 0">
                <StackPanel Orientation="Vertical"
                            VerticalAlignment="Top">
                    <TextBlock Text="{Binding Tag}" Margin="5 0 0 0" FontWeight="SemiBold"  />
                    <TextBlock Text="{Binding Type}" Margin="5 0 0 0"/>
                    <!--Area Properties-->
                    <StackPanel Orientation="Horizontal" 
                                Visibility="Visible">
                        <TextBlock Text="" Margin="5 0 0 0"/>
                        <TextBlock Text="{Binding NemaRating}"/>
                        <TextBlock Text=", " Margin="0 0 0 0"/>
                        <TextBlock Text="{Binding AreaClassification}" />
                    </StackPanel>

                    <!--Extra Details-->
                    <StackPanel x:Name="extraDetails"
                                    Orientation="Vertical">
                        <TextBlock Text="{Binding Area.Tag}"
                                   FontWeight="SemiBold"
                                   Grid.Column="2"
                                   Margin="5 0 0 0"
                                   Visibility="Collapsed"/>

                        <!--Area Properties-->
                        <StackPanel Orientation="Horizontal" 
                                            Visibility="Collapsed">
                            <TextBlock Text="" Margin="5 0 0 0"/>
                            <TextBlock Text="{Binding NemaRating}" Margin="0 0 0 0"/>
                            <TextBlock Text=", " Margin="0 0 0 0"/>
                            <TextBlock Text="{Binding AreaClassification}" Margin="0 0 0 0"/>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>
            </Border>
            <Border BorderThickness="0 0 1 0" HorizontalAlignment="Center"
                    Grid.Row="2" Grid.Column="1"
                    Margin="1 0 0 0"
                    Height="{Binding ActualHeight, ElementName=extraDetails}">
            </Border>
        </Grid>
    </DataTemplate>


    <!--DISCONNECT-->
    <!--Uds-->
    <DataTemplate x:Key="UdsTemplate">
        <Grid Style="{StaticResource GraphicHighlightStyle}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="18.5"/>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            
            <!--Graphic-->
            <Border BorderThickness="1" HorizontalAlignment="Center"
                    Grid.Row="0" Grid.Column="1"
                    Width="40" Height="55"
                    >
                <Canvas Grid.Column="1" Width="50" Height="55">
                    <Line X1="20" X2="20" Y1="0" Y2="15"
                                  Stroke="Black" StrokeThickness="1"/>
                    <Line X1="10" X2="20" Y1="15" Y2="35"
                                  Stroke="Black" StrokeThickness="1"/>
                    <Line X1="20" X2="20" Y1="35" Y2="55"
                                  Stroke="Black" StrokeThickness="1"/>
                </Canvas>
            </Border>

            <!--Data-->
            <Border Grid.Row="0" Grid.Column="2"
                    Grid.RowSpan="2"
                    CornerRadius="{StaticResource BorderHighlightCornerRadius}"
                    Padding="0 0 5 0">
                <StackPanel Orientation="Vertical"
                            VerticalAlignment="Top">
                    <TextBlock Text="{Binding Tag}"
                                FontWeight="SemiBold"
                                Grid.Column="2"
                                Margin="5 0 0 0"/>
                    <TextBlock Text="{Binding FrameAmps, StringFormat={}{0:N0} A}"
                                Grid.Column="2"
                                Margin="5 0 0 0"/>
                    <!--Area Properties-->
                    <StackPanel Orientation="Horizontal" 
                                Visibility="Visible">
                        <TextBlock Text="" Margin="5 0 0 0"/>
                        <TextBlock Text="{Binding NemaRating}"/>
                        <TextBlock Text=", " Margin="0 0 0 0"/>
                        <TextBlock Text="{Binding AreaClassification}" />
                    </StackPanel>

                    <!--Extra Details-->
                    <StackPanel x:Name="extraDetails"
                                Orientation="Vertical">
                        <!--Area Tag-->
                        <TextBlock Text="{Binding Area.Tag}"
                                   FontWeight="SemiBold"
                                   Grid.Column="2"
                                   Margin="5 0 0 0"
                                   Visibility="Collapsed"/>
                        <!--Area Properties-->
                        <StackPanel Orientation="Horizontal" 
                                    Visibility="Collapsed">
                            <TextBlock Text="" Margin="5 0 0 0"/>
                            <TextBlock Text="{Binding NemaRating}"/>
                            <TextBlock Text=", " Margin="0 0 0 0"/>
                            <TextBlock Text="{Binding AreaClassification}" />
                        </StackPanel>
                    </StackPanel>
                </StackPanel>
            </Border>
            
            <!--Extension of Cable for Extra Data-->
            <Border BorderThickness="0 0 1 0" HorizontalAlignment="Center"
                    Grid.Row="2" Grid.Column="1"
                    Margin="1 0 0 0"
                    Height="{Binding ActualHeight, ElementName=extraDetails}">
            </Border>
        </Grid>
    </DataTemplate>

    <!--Fds-->
