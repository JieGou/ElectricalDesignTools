<ResourceDictionary
                    
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    
                    
                    xmlns:templateSelectors="clr-namespace:WpfUI.TemplateSelectors"
                    xmlns:SingleLineLoadGraphics="clr-namespace:WpfUI.Templates.SingleLineLoadGraphics"
                    xmlns:edtDteqModels="clr-namespace:EDTLibrary.Models.DistributionEquipment;assembly=EDTLibrary"
                    xmlns:edtLoadModels="clr-namespace:EDTLibrary.Models.Loads;assembly=EDTLibrary"
                    
                    xmlns:singleLineSubViews="clr-namespace:WpfUI.Views.Electrical.SingleLineSubViews"
                    
                    
                    >

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="TemplateStyles/SingleLineGraphicsResources.xaml"/>
    </ResourceDictionary.MergedDictionaries>


        <!--#region Templates-->

    <!-- MCC BUCKET / OCPD -->

    <!--EmptyTemplate-->
    <DataTemplate x:Key="EmptyTemplate">
        <StackPanel Orientation="Vertical">
            <Grid Background="Transparent"
                  Style="{StaticResource GraphicHighlightStyle}"
                  Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0"/>
                </Grid.ColumnDefinitions>
            </Grid>
        </StackPanel>
    </DataTemplate>


    <!--Breaker Template-->
    <DataTemplate x:Key="BreakerTemplate">
        <StackPanel Orientation="Vertical">

            <Grid Background="Transparent"
                  Style="{StaticResource GraphicHighlightStyle}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="26"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>
                
                
                <Border BorderThickness="0"
                        Grid.Column="1"
                        >
                    <Border.CornerRadius>
                        <CornerRadius TopLeft="{StaticResource HighlightCorerRadius}"
                                      BottomLeft="{StaticResource HighlightCorerRadius}"
                                      TopRight="0"
                                      BottomRight="0"/>
                    </Border.CornerRadius>
                    
                    <Canvas Grid.Column="1"
                            Width="35" Height="90">
                        <Line Stroke="Black" StrokeThickness="1" X1="13.5" Y1="0" X2="13.5" Y2="6"/>
                        <Ellipse Stroke="Black" StrokeThickness="1" Canvas.Left="10" Canvas.Top="6" Height="7" Width="7"/>
                        <Path Stroke="Black" StrokeThickness="1" Data="M 20,10 A 4,5 0 0 1 20, 35" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Canvas.Left="10" Canvas.Top="32" Height="7" Width="7"/>
                        <Line Stroke="Black" StrokeThickness="1" X1="13.5" Y1="39" X2="13.5" Y2="90"/>
                    </Canvas>
                    
                </Border>

                <Border BorderThickness="0"
                        Grid.Column="2"                      
                        Padding="0 0 5 0">
                    <Border.CornerRadius>
                        <CornerRadius TopLeft="0"
                                      BottomLeft="0"
                                      TopRight="{StaticResource HighlightCorerRadius}"
                                      BottomRight="{StaticResource HighlightCorerRadius}"/>
                    </Border.CornerRadius>

                    <Grid Grid.Column="2" Style="{StaticResource GraphicHighlightStyle}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="27"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <StackPanel Orientation="Vertical"
                                Grid.Row="1">
                            <TextBlock Text="{Binding PdSizeTrip, StringFormat={}{0:N0} AT}" />
                            <Border BorderThickness="0 0 0 1"/>
                            <TextBlock Text="{Binding PdSizeFrame, StringFormat={}{0:N0} AF}" />
                        </StackPanel>
                    </Grid>
                </Border>

            </Grid>

            <!--Dashed Border-->
            <Border BorderThickness="0,0,0,1">
                <Border.BorderBrush>
                    <DrawingBrush Viewport="0,0,8,8" ViewportUnits="Absolute" TileMode="Tile">
                        <DrawingBrush.Drawing>
                            <DrawingGroup>
                                <GeometryDrawing Brush="Black">
                                    <GeometryDrawing.Geometry>
                                        <GeometryGroup>
                                            <RectangleGeometry Rect="0,0,50,50" />
                                            <RectangleGeometry Rect="50,50,50,50" />
                                        </GeometryGroup>
                                    </GeometryDrawing.Geometry>
                                </GeometryDrawing>
                            </DrawingGroup>
                        </DrawingBrush.Drawing>
                    </DrawingBrush>
                </Border.BorderBrush>
            </Border>
        </StackPanel>
    </DataTemplate>

    <!--FVNR Template-->
    <DataTemplate x:Key="FvnrTemplate">
        <StackPanel Orientation="Vertical">

            <Grid Background="Transparent"
                  Style="{StaticResource GraphicHighlightStyle}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="26"/>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>

                <Border BorderThickness="0"
                        Grid.Column="1"
                        Padding="5 0"
                        Margin="-5 0">
                    <Border.CornerRadius>
                        <CornerRadius TopLeft="{StaticResource HighlightCorerRadius}"
                                      BottomLeft="{StaticResource HighlightCorerRadius}"
                                      TopRight="0"
                                      BottomRight="0"/>
                    </Border.CornerRadius>

                    <Canvas Grid.Column="1"
                            Width="35" Height="90">
                        <!--Breaker-->
                        <Line Stroke="Black" StrokeThickness="1" X1="13.5" Y1="0" X2="13.5" Y2="6"/>
                        <Ellipse Stroke="Black" StrokeThickness="1" Canvas.Left="10" Canvas.Top="6" Height="7" Width="7"/>
                        <Path Stroke="Black" StrokeThickness="1" Data="M 20,10 A 4,5 0 0 1 20, 35" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Canvas.Left="10" Canvas.Top="32" Height="7" Width="7"/>
                        <Line Stroke="Black" StrokeThickness="1" X1="13.5" Y1="39" X2="13.5" Y2="45"/>

                        <!--Overload-->
                        <Border BorderThickness="1" Background="Transparent"
                            Canvas.Left="0" Canvas.Top="45" Width="27" Height="15"
                                >
                            <TextBlock Text="O/L" HorizontalAlignment="Center" VerticalAlignment="Center"
                                       Margin="0 0 0 1"/>
                        </Border>
                        <!--<Path Stroke="Black" StrokeThickness="1" Data="M 13,45 A 5,5 0 0 1 13.5, 55" />
                        <Path Stroke="Black" StrokeThickness="1" Data="M 13.5,55 A 5,5 0 0 0 14, 65" /> -->

                        <!--Contactor-->
                        <Line Stroke="Black" StrokeThickness="1" X1="13.5" Y1="60" X2="13.5" Y2="70"/>
                        <Line Stroke="Black" StrokeThickness="1" X1="0" Y1="70" X2="27" Y2="70"/>
                        <Line Stroke="Black" StrokeThickness="1" X1="0" Y1="78" X2="27" Y2="78"/>
                        <Line Stroke="Black" StrokeThickness="1" X1="13.5" Y1="78" X2="13.5" Y2="90"/>
                    </Canvas>
                </Border>

                <Border BorderThickness="0"
                            Grid.Column="2"
                        Padding="0 0 5 0"
                        Margin="5 0">
                    <Border.CornerRadius>
                        <CornerRadius TopLeft="0"
                                      BottomLeft="0"
                                      TopRight="{StaticResource HighlightCorerRadius}"
                                      BottomRight="{StaticResource HighlightCorerRadius}"/>
                    </Border.CornerRadius>
                    
                    <Grid Grid.Column="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="27"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>


                        <!--Breaker Size-->
                        <StackPanel Orientation="Vertical"
                                Grid.Row="1">
                            <TextBlock Text="{Binding PdSizeTrip, StringFormat={}{0:N0} AT}" />
                            <Border BorderBrush="Black" BorderThickness="0 0 0 1"/>
                            <TextBlock Text="{Binding PdSizeFrame, StringFormat={}{0:N0} AF}" />
                        </StackPanel>

                        <!--Starter Size-->
                        <StackPanel Orientation="Horizontal"
                                    Grid.Row="3">
                            <TextBlock Text="Size: "/>
                            <TextBlock Text="{Binding StarterSize}" FontWeight="SemiBold"/>
                        </StackPanel>
                    </Grid>
                </Border>

            </Grid>

            <!--Dashed Border-->
            <Border BorderThickness="0,0,0,1">
                <Border.BorderBrush>
                    <DrawingBrush Viewport="0,0,8,8" ViewportUnits="Absolute" TileMode="Tile">
                        <DrawingBrush.Drawing>
                            <DrawingGroup>
                                <GeometryDrawing Brush="Black">
                                    <GeometryDrawing.Geometry>
                                        <GeometryGroup>
                                            <RectangleGeometry Rect="0,0,50,50" />
                                            <RectangleGeometry Rect="50,50,50,50" />
                                        </GeometryGroup>
                                    </GeometryDrawing.Geometry>
                                </GeometryDrawing>
                            </DrawingGroup>
                        </DrawingBrush.Drawing>
                    </DrawingBrush>
                </Border.BorderBrush>
            </Border>
        </StackPanel>
    </DataTemplate>

